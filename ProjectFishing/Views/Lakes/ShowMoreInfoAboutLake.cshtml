    <script src="~/Scripts/angular-ymaps.js"></script>
    <script>


        var mainApp = angular.module("mainApp", ['ymaps']);
        var idViewBag = @ViewBag.IdLake;
        mainApp.controller("mainController", function ($scope, $http) {
            $scope.LakeModel;
            $scope.IdLake = idViewBag;
            $scope.Longitude;
            $scope.Latitude;
            $scope.Coords = [];
            $scope.Zoom = 13;
            //let map;



            $http({
                method: 'GET',
                url: '/Lakes/GetLakeById/'+$scope.IdLake,

            })
                .then(function succes(responce) {
                    $scope.LakeModel = responce.data;
                     console.log($scope.LakeModel);
                    $scope.Longitude = $scope.LakeModel.Post.Longitude;

                    $scope.Latitude = $scope.LakeModel.Post.Latitude;
                    $scope.Coords = [$scope.Longitude, $scope.Latitude];
                    console.log($scope.Coords);

                });

        });
    </script>
