    <div class="container">
        <!-- Row -->
        <div class="row">
            <!-- Content Area -->
            <div class="content-area content-area75 blogpost col-md-9 col-sm-8">
                <article class="type-post blog-onecolumn format-image">
                    <div class="entry-cover ">
                        <div>
                            <div class="photo-slider container-fluid no-padding">
                                <!-- Main Carousel -->
                                <div id="main-carousel" class="carousel slide carousel-fade col-offset-2">
                                    <div class="carousel-inner" role="listbox">
                                        <div class="item active">
                                            <div class="slider-image">
                                                <img style="height:500px" src="~/images/ForViews/Lakes/{{LakeModel.Images[0].Name}}" alt="slider-1">
                                            </div>
                                        </div>
                                        <div class="item" ng-if="LakeModel.Images[1]!=null">
                                            <div class="slider-image">
                                                <img style="height:500px" src="~/images/ForViews/Lakes/{{LakeModel.Images[1].Name}}" alt="slider-1">
                                            </div>
                                        </div>
                                        <div class="item " ng-if="LakeModel.Images[2]!=null">
                                            <div class="slider-image">
                                                <img style="height:500px" src="~/images/ForViews/Lakes/{{LakeModel.Images[2].Name}}" alt="slider-1">
                                            </div>
                                        </div>
                                        <a class="left carousel-control" href="#main-carousel" role="button" data-slide="prev">
                                            <i class="fa fa-angle-left"></i>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                        <a class="right carousel-control" href="#main-carousel" role="button" data-slide="next">
                                            <i class="fa fa-angle-right"></i>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </div><!-- Main Carousel /-  -->
                                </div><!-- Photo Slider /- -->

                            </div>


                            <div class="entry-meta">
                                <div class="post-date">
                                    <p><span>{{LakeModel.Post.Name}}</span></p>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="entry-title">
                        <h3>Описание: {{LakeModel.Post.Description}}</h3>
                    </div>
                    <div class="entry-content">
                        <p>Локация: {{LakeModel.Post.Location}}</p>
                        <p>Температура воды: {{LakeModel.Post.WaterTemperatureBySeasons}}</p>
                        <p>Качество воды: {{LakeModel.Post.QualityOfWater}}</p>



                    </div>
                </article>



            </div><!-- Content Area /- -->
            @*@{Html.RenderPartial("CategoriesPartial");}*@
        </div><!-- Row /- -->
        <div center="Coords" style="width:500px; height:500px;">
            <yandex-map center="center" zoom="13" style="width:500px; height:500px;">
                <ymap-marker coordinates="Coords"></ymap-marker>
            </yandex-map>
        </div>
    </div>

